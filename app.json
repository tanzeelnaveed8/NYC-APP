{
  "expo": {
    "name": "NYC Precinct",
    "slug": "nycprecinct",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "scheme": "nycprecinct",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#0A1929"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.nycprecinct.app",
      "config": {
        "googleMapsApiKey": "AIzaSyAy6Un1PzgY5BgUNwgbch9dES5yE9En96I"
      },
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "We need your location to show your current precinct on the map."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0A1929"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.nycprecinct.app",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyAy6Un1PzgY5BgUNwgbch9dES5yE9En96I"
        }
      },
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-sqlite",
      "expo-font",
      "expo-location"
    ]
  }
}
